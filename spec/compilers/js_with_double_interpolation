component Main {
  fun test (input : String) : String {
    input
  }

  fun render : String {
    `      "Hello" + #{test(`"World!"`)}   `
  }
}
--------------------------------------------------------------------------------
class A extends _C {
  a(b) {
    return b
  }

  render() {
    return "Hello" + this.a("World!")
  }
}

A.displayName = "Main"
