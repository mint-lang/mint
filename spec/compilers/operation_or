type Result(error, value) {
  Err(error)
  Ok(value)
}

type Maybe(value) {
  Nothing
  Just(value)
}

component Main {
  fun render : String {
    Maybe.Nothing or "Hello"
  }
}
--------------------------------------------------------------------------------
import {
  variant as A,
  or as B
} from "./runtime.js";

export const
  C = A(1, `Err`),
  D = A(1, `Just`),
  E = A(0, `Nothing`),
  F = A(1, `Ok`),
  G = () => {
    return B(E, C, new E(), `Hello`)
  };
