type Result(error, value) {
  Err(error)
  Ok(value)
}

type Maybe(value) {
  Nothing
  Just(value)
}

component Main {
  fun render : String {
    Maybe.Nothing or "Hello"
  }
}
--------------------------------------------------------------------------------
import {
  variant as A,
  or as B
} from "./runtime.js";

export const
  C = A(0),
  D = A(1),
  E = A(1),
  F = A(1),
  G = () => {
    return B(C, E, new C(), `Hello`)
  };
