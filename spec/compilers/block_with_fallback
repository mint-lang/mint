type Maybe(a) {
  Nothing
  Just(a)
}

component Main {
  fun render : Html {
    let Just(_) =
      Maybe.Just("")

    let Just(_) =
      Maybe.Just("")

    <span>"Hello"</span>
  } or <span>"Fallback"</span>
}
--------------------------------------------------------------------------------
import {
  createElement as B,
  destructure as C,
  newVariant as D,
  pattern as E,
  variant as A
} from "./runtime.js";

export const
  F = A(1, `Just`),
  G = A(0, `Nothing`),
  H = () => {
    const a = () => {
      return B(`span`, {}, [`Fallback`])
    };
    const b = C(D(F)(``), E(F, [null]));
    if (b === false) {
      return a()
    };
    const [] = b;
    const c = C(D(F)(``), E(F, [null]));
    if (c === false) {
      return a()
    };
    const [] = c;
    return B(`span`, {}, [`Hello`])
  };
