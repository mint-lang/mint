type Maybe(a) {
  Nothing
  Just(a)
}

suite "Test" {
  test "X" {
    <button as button/>

    button == Maybe.Nothing
  }
}
--------------------------------------------------------------------------------
import {
  createElement as D,
  setTestRef as E,
  testRunner as B,
  compare as F,
  variant as A,
  signal as C
} from "./runtime.js";

export const
  G = A(1, `Just`),
  H = A(0, `Nothing`);

export default () => {
  new B([{
    tests: [{
      proc: () => {
        const a = C(new H());
        return (() => {
          D(`button`, {
            ref: E(a, G, H)
          });
          return F(a.value, new H())
        })()
      },
      location: {"start":[7,2],"end":[11,3],"filename":"compilers/test_with_reference"},
      name: `X`
    }],
    location: {"start":[6,0],"end":[12,1],"filename":"compilers/test_with_reference"},
    name: `Test`
  }], {}, ``, ``)
};
