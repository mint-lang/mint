component Main {
  fun render : String {
    case ({"A", 0, true}) {
      {"A", 0, false} =>
        "B"

      {"A", 0, true} =>
        "A"

      {a, b, c} =>
        a
    }
  }
}
--------------------------------------------------------------------------------
import {
  patternVariable as C,
  match as B
} from "./runtime.js";

export const A = () => {
  return B([
    `A`,
    0,
    true
  ], [
    [
      [
        `A`,
        0,
        false
      ],
      () => {
        return `B`
      }
    ],
    [
      [
        `A`,
        0,
        true
      ],
      () => {
        return `A`
      }
    ],
    [
      [
        C,
        C,
        C
      ],
      (a, b, c) => {
        return a
      }
    ]
  ])
};
