type Result(error, value) {
  Err(error)
  Ok(value)
}

type X.Y {
  blah : String
}

type X {
  name : String,
  y: X.Y
}

component Main {
  fun render : String {
    (decode as X)(`null`)

    ""
  }
}
--------------------------------------------------------------------------------
import {
  decodeString as C,
  decoder as B,
  variant as A
} from "./runtime.js";

export const
  D = A(1),
  E = A(1),
  a = B({
    blah: C(E, D)
  }, E, D),
  b = B({
    name: C(E, D),
    y: a
  }, E, D),
  F = () => {
    (b)((null));
    return ``
  };
