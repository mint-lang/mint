component Main {
  style one {
    color: red;
  }

  style two(color : String) {
    color: #{color};
  }

  fun render : Html {
    <div::one::two("blue")>
    </div>
  }
}
--------------------------------------------------------------------------------
class A extends _C {
  $Main_two_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE(a) {
    const _ = {
      [`--a-a`]: a
    };

    return _;
  }

  render() {
    return _h("div", {
      className: `Main_one_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE Main_two_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE`,
      style: _style([this.$Main_two_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE(`blue`)])
    });
  }
};

A.displayName = "Main";

_insertStyles(`
.Main_one_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE {
  color: red;
}

.Main_two_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE {
  color: var(--a-a);
}
`);
