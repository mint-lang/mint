component Main {
  style one {
    color: red;
  }

  style two {
    color: blue;
  }

  fun render : Html {
    <div::one::two>
    </div>
  }
}
--------------------------------------------------------------------------------
class A extends _C {
  render() {
    return _h("div", {
      className: `one_HASH_ID two_HASH_ID`
    });
  }
};

A.displayName = "Main";

_insertStyles(`
.one_HASH_ID {
  color: red;
}

.two_HASH_ID {
  color: blue;
}
`);
