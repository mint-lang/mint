component Main {
  style one {
    color: red;
  }

  style two {
    color: blue;
  }

  fun render : Html {
    <div::one::two>
    </div>
  }
}
--------------------------------------------------------------------------------
class A extends _C {
  render() {
    return _h("div", {
      className: `Main_one_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE Main_two_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE`
    });
  }
};

A.displayName = "Main";

_insertStyles(`
.Main_one_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE {
  color: red;
}

.Main_two_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE {
  color: blue;
}
`);
