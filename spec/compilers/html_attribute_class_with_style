component Main {
  style base {
    width: 100%;
  }

  fun render : Html {
    <div::base class="something">
    </div>
  }
}
--------------------------------------------------------------------------------
class A extends _C {
  render() {
    return _h("div", {
      className: `something` + ` Main_base_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE`
    });
  }
};

A.displayName = "Main";

_insertStyles(`
.Main_base_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE {
  width: 100%;
}
`);
