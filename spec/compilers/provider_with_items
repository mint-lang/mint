type Subscription {
  a : Bool,
  b : Bool
}

provider Provider : Subscription {
  const NAME = "hello"

  state a : String = ""

  get b : String {
    a
  }

  fun update : Promise(Void) {
    await void
  }

  fun name : String {
    NAME
  }
}

component Main {
  use Provider {
    a: true,
    b: false
  }

  fun render {
    <div/>
  }
}
--------------------------------------------------------------------------------
import {
  createProvider as B,
  createElement as D,
  signal as A,
  useId as C
} from "./runtime.js";

export const
  a = `hello`,
  b = () => {
    return a
  },
  c = A(``),
  d = () => {
    return c.value
  },
  e = new Map(),
  E = B(e, async () => {
    return await null
  }),
  F = () => {
    const f = C();
    E(f, () => {
      return {
        a: true,
        b: false
      }
    });
    return D(`div`, {})
  };
