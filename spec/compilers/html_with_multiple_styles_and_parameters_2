component Main {
  style one(color: String) {
    color: #{color};
  }

  style two(color : String) {
    color: #{color};
  }

  fun render : Html {
    <div::one("red")::two("blue")>
    </div>
  }
}
--------------------------------------------------------------------------------
class A extends _C {
  $Main_one_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE(a) {
    const _ = {
      [`--a-a`]: a
    };

    return _;
  }

  $Main_two_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE(b) {
    const _ = {
      [`--b-a`]: b
    };

    return _;
  }

  render() {
    return _h("div", {
      className: `Main_one_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE Main_two_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE`,
      style: _style([this.$Main_one_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE(`red`), this.$Main_two_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE(`blue`)])
    });
  }
};

A.displayName = "Main";

_insertStyles(`
.Main_one_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE {
  color: var(--a-a);
}

.Main_two_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE {
  color: var(--b-a);
}
`);
