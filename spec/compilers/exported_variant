exports:
  - Maybe.Nothing
  - Maybe.Just
================================================================================
type Maybe(a) {
  Nothing
  Just(a)
}

component Main {
  fun render : String {
    Maybe.Just("Hello")
    Maybe.Nothing
    ""
  }
}
--------------------------------------------------------------------------------
import { variant as A } from "./runtime.js";

export const
  B = A(1, `Just`),
  C = A(0, `Nothing`);

export const
  Nothing = C,
  Just = B;
