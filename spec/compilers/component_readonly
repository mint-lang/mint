component Test {
  property readonly : Bool = false

  fun render : Html {
    <div></div>
  }
}

component Main {
  fun render : Html {
    <Test readonly={true}/>
  }
}
--------------------------------------------------------------------------------
import { createElement as B } from "./runtime.js";

export const
  A = ({
    a = false
  }) => {
    return B(`div`, {})
  },
  C = () => {
    return B(A, {
      a: true
    })
  };
