type Maybe(a) {
  Nothing
  Just(a)
}

type Result(error, value) {
  Err(error)
  Ok(value)
}

component Main {
  fun render : String {
    let a =
      Maybe.Just("A")

    if a {
      a
    } else {
      "b"
    }
  }
}
--------------------------------------------------------------------------------
import {
  newVariant as B,
  isThruthy as C,
  variant as A
} from "./runtime.js";

export const
  D = A(1, `Err`),
  E = A(1, `Just`),
  F = A(0, `Nothing`),
  G = A(1, `Ok`),
  H = () => {
    const a = B(E)(`A`);
    return (C(a, E, G) ? (() => {
      let b = a._0;
      return b
    })() : `b`)
  };
