component HtmlAttribute {
  fun render : Html {
    <div ref={\element : Dom.Element => void}>
    </div>
  }
}
--------------------------------------------------------------------------------
class $HtmlAttribute extends Component {
  render() {
    return _createElement("div", {
      "ref": (ref => {
        ref ? ((element) => {
          return null
        }).call(this, ref) : null
      })
    })
  }
}

$HtmlAttribute.displayName = "HtmlAttribute"
