component Main {
  style test {
    &:focus {
      color: red;
    }

    &[someattribute] {
      color: red;
    }

    &.someclass {
      color: red;
    }
  }

  fun render : Html {
    <div::test/>
  }
}
--------------------------------------------------------------------------------
class A extends _C {
  render() {
    return _h("div", {
      className: `Main_test_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE`
    });
  }
};

A.displayName = "Main";

_insertStyles(`
.Main_test_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE:focus {
  color: red;
}

.Main_test_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE[someattribute] {
  color: red;
}

.Main_test_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE.someclass {
  color: red;
}
`);
