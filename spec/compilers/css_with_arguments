component Main {
  style test(color : String) {
    color: #{color};
  }

  fun render : Html {
    <div::test("red")/>
  }
}
--------------------------------------------------------------------------------
class A extends _C {
  $Main_test_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE(a) {
    const _ = {
      [`--a-a`]: a
    };

    return _;
  }

  render() {
    return _h("div", {
      className: `Main_test_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE`,
      style: _style([this.$Main_test_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE(`red`)])
    });
  }
};

A.displayName = "Main";

_insertStyles(`
.Main_test_DO_NOT_USE_MINIMIZED_IN_BUILD_CODE {
  color: var(--a-a);
}
`);
