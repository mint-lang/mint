component Main {
  style test {
    margin: #{margin}px 0px;
  }

  get margin : Number {
    10
  }

  fun render : Html {
    <div::test/>
  }
}
--------------------------------------------------------------------------------
class A extends _C {
  $test_HASH_ID() {
    const _ = {
      [`--a-a`]: this.a + `px 0px`
    };

    return _;
  }

  get a() {
    return 10;
  }

  render() {
    return _h("div", {
      className: `test_HASH_ID`,
      style: _style([this.$test_HASH_ID()])
    });
  }
};

A.displayName = "Main";

_insertStyles(`
.test_HASH_ID {
  margin: var(--a-a);
}
`);
