--------------------------------------------------------------------------------
type Subscription {
  a : Bool
}

provider Provider : Subscription {
  /* Comment */
  const NAME = "hello"

  state a : String = ""

  get b : String {
    a
  }

  fun name : String {
    NAME
  }
}
