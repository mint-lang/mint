record A {
  a : String
}

module Test {
  fun test : A {
    let x =
      { a : "Blah" }

    {x|a:"Hello"}
  }
}
--------------------------------------------------------------------------------
record A {
  a : String
}

module Test {
  fun test : A {
    let x =
      { a: "Blah" }

    { x | a: "Hello" }
  }
}
