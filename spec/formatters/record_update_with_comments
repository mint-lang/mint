type A {
  a : String
}

module Test {
  fun test : A {
    let x =
      { a : "Blah" }

    { x|
      // Comment
      a:"Hello"
      // End Comment
    }
  }
}
--------------------------------------------------------------------------------
type A {
  a : String
}

module Test {
  fun test : A {
    let x =
      { a: "Blah" }

    { x |
      // Comment
      a: "Hello"
      // End Comment
    }
  }
}
