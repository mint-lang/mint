record A {
  a : String,
  b : Bool
}

module Test {
  fun test : A {
    let x =
      { a : "Blah", b : false }

    {x|a:"Hello",b:true}
  }
}
--------------------------------------------------------------------------------
record A {
  a : String,
  b : Bool
}

module Test {
  fun test : A {
    let x =
      {
        a: "Blah",
        b: false
      }

    { x |
      a: "Hello",
      b: true
    }
  }
}
