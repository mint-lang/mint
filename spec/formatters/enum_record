enum A {
  B(name : String, color : String)
  C
}

component Main {
  fun render : String {
    case A::B(name: "Joe", color: "Blue") {
      A::B(name, color) => ""
      A::C => ""
    }
  }
}
--------------------------------------------------------------------------------
enum A {
  B(name : String, color : String)
  C
}

component Main {
  fun render : String {
    case A::B(name: "Joe", color: "Blue") {
      A::B(name, color) => ""
      A::C => ""
    }
  }
}
