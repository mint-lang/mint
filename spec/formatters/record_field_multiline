record Test {
  a : String
}

module Test {
  fun get : Test {
    {
      a:
        "HelloHelloHelloHelloHelloHelloHelloHelloHelloHelloHelloH" \
        "elloHelloHelloHelloHelloHelloHelloHello"
    }
  }
}
