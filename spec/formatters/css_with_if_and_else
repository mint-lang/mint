component A {
  styletest{background: red;color: blue;
    if(true){color:yellow;}else{color:brown;}}

  fun render : Html {
    <div/>
  }
}
--------------------------------------------------------------------------------
component A {
  style test {
    background: red;
    color: blue;

    if true {
      color: yellow;
    } else {
      color: brown;
    }
  }

  fun render : Html {
    <div/>
  }
}
