component Main {
  fun render : String {
    return "YES"
    "NO"
  }
}
---------------------------------------------------------ReturnCallTypeMismatch
enum Test {
  A(String)
  B(String)
}

component Main {
  fun render : String {
    let Test::B(x) =
      Test::A("Hello") or return 0

    x
  }
}
--------------------------------------------------------StatementReturnRequired
component Main {
  fun render : String {
    let [a, b] = []

    ""
  }
}
