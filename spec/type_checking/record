record Test {
  a : String,
  b : Number
}

component Main {
  fun test : Test {
    {
      a = "Hello",
      b = 0
    }
  }

  fun render : String {
    ""
  }
}
-----------------------------------------------------------------RecordWithHoles
record Test {
  a : Array(a),
  b : Number
}
--------------------------------------------------------------------------------
component Main {
  fun test : Object {
    encode {
      a = "Hello",
      b = "Blah"
    }
  }

  fun render : String {
    ""
  }
}
