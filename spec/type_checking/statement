component Main {
  fun render : String {
    let x = "hello"
  }
}
---------------------------------------------------------------StatementNotTuple
component Main {
  fun render : String {
    let {x, y, z} = "hello"
  }
}
----------------------------------------------------------StatementTupleMismatch
component Main {
  fun render : String {
    let {x, y, z} = {"hello", "a"}
  }
}
