component Main {
  fun render : String {
    let x = "hello"
  }
}
---------------------------------------------------------------CaseBranchNotTuple
component Main {
  fun render : String {
    let {x, y, z} = "hello"
  }
}
----------------------------------------------------------CaseBranchTupleMismatch
component Main {
  fun render : String {
    let {x, y, z} = {"hello", "a"}
  }
}
