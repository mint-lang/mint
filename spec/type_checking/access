record Blah1 {
  blah : String
}

record Blah {
  blah : Blah1
}

component Main {
  fun render : String {
    blah =
      { blah = { blah = "Helllo" } }

    blah.blah.blah
  }
}
-----------------------------------------------------------------AccessNotRecord
component Main {
  fun render : Bool {
    blah = ""

    blah.blah.blah
  }
}
-----------------------------------------------------------------AccessNotRecord
record Blah {
  blah : String
}

component Main {
  fun render : Bool {
    blah =
      { blah = "Blah" }

    blah.blah.blah
  }
}
-------------------------------------------------------------AccessFieldNotFound
record Blah {
  blah : String
}

component Main {
  fun render : Bool {
    blah =
      { blah = "Hello" }

    blah.blaha
  }
}
