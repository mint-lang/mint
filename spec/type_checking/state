component Main {
  state b : String = "Hello"
  state a : Number = 0

  fun render : Html {
    <div/>
  }
}
---------------------------------------------------------------StateTypeMismatch
component Main {
  state b : String = 0

  fun render : Html {
    <div/>
  }
}
---------------------------------------------------------------StateTypeMismatch
record A {
  a : String
}

component Main {
  state name : Test = { a: "Hello" }

  fun render : Html {
    <div/>
  }
}
