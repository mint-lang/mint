store Test {
  state a : String = ""

  fun b : Promise(Void) {
    next { a: "Blah" }
  }
}

component Main {
  connect Test exposing { a, b }

  fun render : String {
    ""
  }
}
-----------------------------------------------------------NextCallStateNotFound
store Test {
  state a : String = ""

  fun b : Promise(Never, Void) {
    next { b: "Blah" }
  }
}

component Main {
  connect Test exposing { a, b }
}
---------------------------------------------------------StoreEntityNameConflict
