// Module Description
module Test {
  // Constant Description
  const CONSTANT = "TEST"

  // Function Description
  fun function (argument1 : String = "Default Value") : String {
    "Hello!"
  }

  // Broken Function Description
  fun brokenFunction (argument1 : String, argument2 : String = "Default Value") : String {
    "Hello!"
  }
}
