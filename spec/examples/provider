record Subscription {
  a : Bool
}

provider Provider : Subscription {
  state observers : Array(Tuple(String, String)) = []

  fun update {
    let currentObservers =
      for item of observers {
        ""
      }

    subscriptions
    true
  }
}

component Main {
  use Provider {
    a: true
  }

  fun render : String {
    ""
  }
}
