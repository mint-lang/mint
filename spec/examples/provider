-------------------------------------------------------------------------------
record Subscription {
    a : Bool
}

provider Provider : Subscription {
  const ONE_PLUS_ONE = 1 + 1
  const TWO = 2

  fun test : Bool {
    ONE_PLUS_ONE == TWO
  }
}
