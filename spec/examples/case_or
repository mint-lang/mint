----------------------------------------missing_variable_in_alternative_pattern
type A {
  B(String)
  C(String)
  D
}

component Main {
  get value : A {
    B("")
  }

  fun render : String {
    case value {
      B(a) | C | D  => "a"
    }
  }
}
-------------------------------------------------------------------------------
type A {
  B
  C
  D
}

component Main {
  get value : A {
    B
  }

  fun render : String {
    case value {
      B | C | D  => "a"
    }
  }
}
-------------------------------------------------------------------------------
type A {
  B(String)
  C(String)
  D(String)
}

component Main {
  get value : A {
    D("WTF")
  }

  fun render : String {
    case value {
      B(a) | C(a) | D(a) => a
    }
  }
}
