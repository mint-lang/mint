---------------------------------------------------------------variable_missing
component Main {
  get a : String {
    "Hello"
  }

  fun b : String {
    c
  }

  fun render : Html {
    b()
  }
}
---------------------------------------------------------------variable_missing
component Main {
  style test(name : String) {
    color: #{name2};
  }

  fun render : Html {
    <div::test("Name")/>
  }
}
--------------------------------------------------------operation_type_mismatch
suite "Suite" {
  const TWO = 2

  test "Test" {
    1 + 1 == THREE
  }
}
--------------------------------------------------------operation_type_mismatch
suite "Suite 2" {
  test "Test" {
    1 + 1 == TWO
  }
}
-----------------------------------------------------------type_variant_missing
type A {
  B(String)
  C
}

component Main {
  fun render : String {
    case (A.D(0)) {
      => ""
    }
  }
}
--------------------------------------------------------------access_not_record
component Main {
  fun render : String {
    case (C.D(0)) {
      => ""
    }
  }
}
--------------------------------------------------------------variable_reserved
component Main {
  fun b(default : String) : String {
    default
  }

  fun render : Html {
    b("")
  }
}
-------------------------------------------------------------------------------
component Main {
  get a : String {
    "Hello"
  }

  fun b : String {
    a
  }

  fun render : String {
    b()
  }
}
