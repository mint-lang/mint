  property render : String = ""

  fun render : Bool {
    true
  }
}
----------------------------------------------component_reference_name_conflict
component Main {
  fun render : Html {
    <div as x>
      <div as x/>
    </div>
  }
}
------------------------------------------component_lifecycle_function_mismatch
