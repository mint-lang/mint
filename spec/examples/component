    ""
  }

  fun render : Html {
    <div/>
  }
}
------------------------------------------------component_exposed_name_conflict
store Test {
  state x : String = ""
}

component A {
  connect Test exposing { x }

  get x : String {
    ""
  }

  fun render : Html {
    <div/>
  }
}
------------------------------------------------component_exposed_name_conflict
store Test {
  state x : String = ""
}

component A {
  connect Test exposing { x as y }

  get y : String {
    ""
  }

  fun render : Html {
    <div/>
  }
}
-------------------------------------------------------------------------------
component Main {
  fun render : Html {
    <div/>
  }
}
-------------------------------------------------------------------------------
global component Test {
  fun c : String {
    ""
  }

  fun b : String {
    c()
  }

  fun render : Html {
    <div></div>
  }
}

component Main {
  fun render : String {
    Test.b()
  }
}
