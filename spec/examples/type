module Test {
  fun test1 : a {
    "I'm a String"
  }

  fun test2 : a {
    "I'm a string too!"
  }

  fun test4 : String {
    test1() + test2()
  }
}
-------------------------------------------------------------------------------
enum A {
  B
  C
}

component Main {
  fun render : String {
    case (A::B) {
      => ""
    }
  }
}
