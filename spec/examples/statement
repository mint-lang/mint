-------------------------------------------------statement_return_type_mismatch
type Test {
  A(String)
  B(String)
}

component Main {
  fun render : String {
    let Test.B(x) =
      Test.A("Hello") or return 0

    x
  }
}
------------------------------------------------------statement_return_required
component Main {
  fun render : String {
    let [a, b] = []

    ""
  }
}
----------------------------------------------------------statement_last_target
component Main {
  fun render : String {
    let a = ""
  }
}
-------------------------------------------------------------------------------
component Main {
  fun render : String {
    let x = "hello"
    ""
  }
}
-------------------------------------------------------------------------------
component Main {
  fun render : String {
    signal x = "hello"
    ""
  }
}
-------------------------------------------------------------------------------
type Test {
  A(String)
}

component Main {
  fun render : String {
    let A(value) = Test.A("Hello")
    let {a, _} = {"a", "b"}

    ""
  }
}
