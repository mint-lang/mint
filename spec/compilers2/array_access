type Maybe(a) {
  Nothing
  Just(a)
}

component Main {
  fun render : String {
    [
      "Hello",
      "Blah",
      "Joe"
    ][1]

    [][1]

    ""
  }
}
--------------------------------------------------------------------------------
import {
  arrayAccess as E,
  variant as B
} from "runtime";

const
  A = B(0),
  C = B(1),
  D = () => {
    E([
      `Hello`,
      `Blah`,
      `Joe`
    ], 1, C, A);
    E([], 1, C, A);
    return ``
  };
