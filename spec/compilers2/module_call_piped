module Test {
  fun a (x : Bool, value : String) : String {
    value
  }

  fun b : String {
    true
    |> Test.a("Lorem ipsum dolor sit amet")
  }
}

component Main {
  fun render : String {
    Test.b()
  }
}
--------------------------------------------------------------------------------
const a = (b, c) => {
  return c
};

const d = () => {
  return a(true, `Lorem ipsum dolor sit amet`)
};

const A = () => {
  return d()
};
