component Main {
  fun render : Html {
    <<#MARKDOWN
    * When open pressing `Esc` closes it.
    MARKDOWN
  }
}
--------------------------------------------------------------------------------
import {
  createElement as B,
  fragment as C
} from "runtime";

export const A = () => {
  return B(C, {}, [B('ul', {}, [B('li', {}, [
    `When open pressing `,
    B('code', {}, [`Esc`]),
    ` closes it.`
  ])])])
};
