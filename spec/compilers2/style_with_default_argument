component Main {
  style test (color : String = "red") {
    color: #{color};
  }

  fun render : Html {
    <div::test>
    </div>
  }
}
--------------------------------------------------------------------------------
---=== /index.js ===---
import {
  createElement as B,
  style as C
} from "runtime";

export const A = () => {
  const a = (b = `red`) => {
    const _ = {
      [`--a-a`]: b
    };
    return _
  };
  return B(`div`, {
    className: `Main_test`,
    style: C([a()])
  })
};

---=== /index.css ===---
.Main_test {
  color: var(--a-a);
}
