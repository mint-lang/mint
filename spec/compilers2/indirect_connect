store Test {
  state a : String = ""

  fun b : String {
    "hello"
  }
}

store A {
  state test : Array(String) = [""]
  state other : String = ""
}

component Main {
  connect Test exposing { a }

  fun render : String {
    A.other
  }
}
--------------------------------------------------------------------------------
import { signal as A } from "runtime";

const
  a = A(``),
  b = () => {
    return `hello`
  },
  c = A(``),
  B = () => {
    return c.value
  };
