store Test {
  state a : String = ""

  fun b : String {
    "hello"
  }
}

store A {
  state test : Array(String) = [""]
  state other : String = ""
}

component Main {
  connect Test exposing { a }

  fun render : String {
    A.other
  }
}
--------------------------------------------------------------------------------
import { signal as A } from "runtime";

const a = A(``);

const b = () => {
  return `hello`
};

const c = A(``);

const B = () => {
  return c.value
};
