component Main {
  fun promise : Promise(String) {
    `` as Promise(String)
  }

  fun promiseTest : Promise(String) {
    await promise()
  }

  fun render : String {
    promiseTest()

    "Hello"
  }
}
--------------------------------------------------------------------------------
import { define as B } from "runtime";

const A = () => {
  const
    a = B('a', () => {
      return undefined
    }),
    b = B('b', async () => {
      return await a()
    });
  b();
  return `Hello`
};
