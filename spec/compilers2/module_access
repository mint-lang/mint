module Test {
  fun a : String {
    "Hello"
  }

  fun b : Function(String) {
    Test.a
  }
}

component Main {
  fun render : String {
    let x =
      Test.b()

    x()
  }
}
--------------------------------------------------------------------------------
const
  a = () => {
    return `Hello`
  },
  b = () => {
    return a
  },
  A = () => {
    const c = b();
    return c()
  };
