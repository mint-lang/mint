component Main {
  state test : String = "Hello"
  state blah : String = "0"

  fun asd : String {
    test + blah
  }

  fun render : String {
    asd()
  }
}
--------------------------------------------------------------------------------
import {
  useSignal as B,
  define as C
} from "runtime";

const A = () => {
  const
    a = B(`Hello`),
    b = B(`0`),
    c = C('c', () => {
      return a.value + b.value
    });
  return c()
};

