import {
  createElement as F,
  variant as B,
  signal as E
} from "runtime";

const
  A = B(1),
  C = B(0),
  D = () => {
    const a = E(new C());
    return F("div", {
      ref: (b) => {
        a.value = b
      }
    })
  };

