enum Maybe(value) {
  Just(value)
  Nothing
}

component Main {
  fun render : Html {
    <div as input>
    </div>
  }
}
--------------------------------------------------------------------------------
import {
  createElement as F,
  variant as B,
  setRef as G,
  useRef as E
} from "runtime";

export const
  A = B(1),
  C = B(0),
  D = () => {
    const a = E(new C());
    return F(`div`, {
      ref: G(a, A)
    })
  };

