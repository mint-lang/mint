component Test {
  property test : String = "Hello"

  fun render : String {
    test
  }
}

component Main {
  fun render : Html {
    <Test/>
  }
}
--------------------------------------------------------------------------------
import { createElement as C } from "runtime";

export const
  A = ({
    a = `Hello`
  }) => {
    return a
  },
  B = () => {
    return C(A, {})
  };
