enum Maybe(a) {
  Nothing
  Just(a)
}

component Main {
  fun render : String {
    Maybe::Nothing or "Hello"
    ""
  }
}
--------------------------------------------------------------------------------
import {
  variant as B,
  or as E
} from "runtime";

const
  A = B(0),
  C = B(1),
  D = () => {
    E(new A()._0, `Hello`);
    return ``
  };
