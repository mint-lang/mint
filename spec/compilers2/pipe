component Main {
  fun render : Html {
    <div>
      <{ (n : String) : String { n }("3") }>
      <{ "3" |> (n : String) : String { n } }>
    </div>
  }
}
--------------------------------------------------------------------------------
import { createElement as B } from "runtime";

export const A = () => {
  return B(`div`, {}, [
    ((a) => {
      return a
    })(`3`),
    ((b) => {
      return b
    })(`3`)
  ])
};
