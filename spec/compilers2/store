store Test {
  state test : String = ""

  fun hello : String {
    "hello"
  }
}

component Main {
  connect Test exposing { test }

  fun render : String {
    test
  }
}
--------------------------------------------------------------------------------
import { signal as A } from "runtime";

const
  a = () => {
    return `hello`
  },
  b = A(``),
  B = () => {
    return b.value
  };
