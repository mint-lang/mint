component Main {
  fun render : String {
    let {result, formatted} =
      @format {
        "HelloHelloHelloHelloHelloHelloHelloHelloHelloHelloHelloH" \
        "Bello"
      }

    result + formatted
  }
}
--------------------------------------------------------------------------------
const A = () => {
  const [
    a,
    b
  ] = [
    `HelloHelloHelloHelloHelloHelloHelloHelloHelloHelloHelloHBello`,
    `"HelloHelloHelloHelloHelloHelloHelloHelloHelloHelloHelloH" \\
"Bello"`
  ];
  return a + b
};
