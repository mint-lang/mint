component Main {
  fun call (a : String, b : Bool) : Bool {
    b
  }

  fun test : Bool {
    call("Hello", true)
  }

  fun render : String {
    test()

    ""
  }
}
--------------------------------------------------------------------------------
import { useFunction as B } from "runtime";

const A = () => {
  const
    a = B((b, c) => {
      return c
    }),
    d = B(() => {
      return a(`Hello`, true)
    });
  d();
  return ``
};

