component Main {
  fun call (a : String, b : Bool) : Bool {
    b
  }

  fun test : Bool {
    call("Hello", true)
  }

  fun render : String {
    test()

    ""
  }
}
--------------------------------------------------------------------------------
import { define as B } from "runtime";

const A = () => {
  const
    a = B('a', (b, c) => {
      return c
    }),
    d = B('d', () => {
      return a(`Hello`, true)
    });
  d();
  return ``
};

