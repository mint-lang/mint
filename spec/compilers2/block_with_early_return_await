enum Test {
  A(String)
  B(String)
}

component Main {
  fun render : String {
    {
      let Test::A(a) =
        await Test::A("Some string...") or return "RETURN"

      a
    }

    ""
  }
}
--------------------------------------------------------------------------------
import {
  patternVariable as H,
  destructure as E,
  newVariant as F,
  pattern as G,
  variant as B
} from "runtime";

const A = B(1);

const C = B(1);

const D = () => {
  (async () => {
    const a = E(await F(A)(`Some string...`), G(A, [H]));
    if (a === false) {
      return `RETURN`
    };
    const [b] = a;
    return b
  })();
  return ``
};
