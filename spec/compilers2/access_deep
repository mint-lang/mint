type Locale {
  level1: Level1
}

type Level1 {
  level2: Level2
}

type Level2 {
  name : String
}

store Settings {
  state locale : Locale = { level1: { level2: { name: "Test" }} }
}

component Main {
  fun render : String {
    Settings.locale.level1.level2.name
  }
}
--------------------------------------------------------------------------------
import { signal as A } from "runtime";

const
  a = A({
    level1: {
      level2: {
        name: `Test`
      }
    }
  }),
  B = () => {
    return a.value.level1.level2.name
  };
