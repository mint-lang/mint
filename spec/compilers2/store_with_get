store Test {
  state test : String = ""

  get hello : String {
    "hello"
  }
}

component Main {
  connect Test exposing { test as xxx }

  fun render : String {
    xxx
    Test.hello
  }
}
--------------------------------------------------------------------------------
import { signal as A } from "./runtime.js";

export const
  a = A(``),
  b = () => {
    return `hello`
  },
  B = () => {
    a.value;
    return b()
  };

