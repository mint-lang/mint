module A {
  fun a : String {
    "Hello"
  }

  fun b : Function(String) {
    a
  }
}

component Main {
  fun render : String {
    let a =
      A.b()

    a()
  }
}
--------------------------------------------------------------------------------
const a = () => {
  return `Hello`
};

const b = () => {
  return a
};

const A = () => {
  const c = b();
  return c()
};
