record Test {
  nested : Nested,
  name : String
}

type Nested {
  name : String
}

component Main {
  fun render : String {
    encode { name: "Hello", nested: { name: "Test" } }

    ""
  }
}
--------------------------------------------------------------------------------
import {
  identity as B,
  encoder as A
} from "runtime";

const
  a = A({
    name: B
  }),
  b = A({
    nested: a,
    name: B
  }),
  C = () => {
    b({
      name: `Hello`,
      nested: {
        name: `Test`
      }
    });
    return ``
  };
