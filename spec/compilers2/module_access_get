store Test {
  get a : String {
    "Hello"
  }

  fun b : String {
    Test.a
  }
}

component Main {
  fun render : String {
    Test.b()
  }
}
--------------------------------------------------------------------------------
import { computed as A } from "runtime";

const
  a = A(() => {
    return `Hello`
  }),
  b = () => {
    return a.value
  },
  B = () => {
    return b()
  };
