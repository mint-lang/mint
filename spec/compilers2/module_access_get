store Test {
  get a : String {
    "Hello"
  }

  fun b : String {
    Test.a
  }
}

component Main {
  fun render : String {
    Test.b()
  }
}
--------------------------------------------------------------------------------
const
  a = () => {
    return `Hello`
  },
  b = () => {
    return a()
  },
  A = () => {
    return b()
  };
