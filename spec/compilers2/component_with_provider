  a = new Map(),
  A = B(a, () => {
    return null
  }),
  C = () => {
    const b = D();
    A(b, () => {
      return (false ? {
        moves: (c) => {
          return null
        },
        ups: (d) => {
          return null
        }
      } : null)
    });
    return E(`div`, {})
