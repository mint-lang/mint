enum Test(a) {
  X
  Y
  Z(Res(a, Number))
}

enum Res(error, value) {
  Error(error)
  Ok(value)
  Other(error, value)
}

component Main {
  fun render : String {
    Test::X
    Res::Other("", "")
    Test::Z(Res::Error(""))
    ""
  }
}
--------------------------------------------------------------------------------
import {
  newVariant as I,
  variant as B
} from "runtime";

const
  A = B(0),
  C = B(0),
  D = B(1),
  E = B(1),
  F = B(1),
  G = B(2),
  H = () => {
    new A();
    I(G)(``, ``);
    I(D)(I(E)(``));
    return ``
  };
