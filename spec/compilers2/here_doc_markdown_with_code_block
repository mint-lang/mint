component Main {
  fun render : Html {
    <<#MARKDOWN(highlight)
    Text

    ```mint
    module Time {}
    ```

    Text
    MARKDOWN
  }
}
--------------------------------------------------------------------------------
import {
  createElement as B,
  fragment as C
} from "runtime";

const A = () => {
  return B(C, {}, [
    B('p', {}, [`Text`]),
    B('pre', {}, [B('code', {
      class: "language-mint"
    }, [B('span', {
      className: "line"
    }, [
      B('span', {
        className: "keyword"
      }, [`module`]),
      ` `,
      B('span', {
        className: "type"
      }, [`Time`]),
      ` {}`
    ])])]),
    B('p', {}, [`Text`])
  ])
};
