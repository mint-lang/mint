record Test {
  test : String
}

provider Test : Test {
  fun update : Promise(Void) {
    await void
  }

  fun print (a : String) : String {
    a
  }
}

component Main {
  use Test {
    test: ""
  }

  fun render : String {
    Test.subscriptions
    Test.print("a")
  }
}
--------------------------------------------------------------------------------
import {
  createProvider as C,
  useProviders as E,
  signal as A
} from "runtime";

const
  a = A([]),
  b = (c) => {
    return c
  },
  B = C(a, async () => {
    return await null
  }),
  D = () => {
    E([[() => {
      return B({
        test: ``
      })
    }]]);
    a.value;
    return b(`a`)
  };
