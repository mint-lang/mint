record Test {
  test : String
}

provider Test : Test {
  fun update : Promise(Void) {
    await void
  }

  fun print (a : String) : String {
    a
  }
}

component Main {
  use Test {
    test: ""
  }

  fun render : String {
    Test.subscriptions
    Test.print("a")
  }
}
--------------------------------------------------------------------------------
import {
  createProvider as C,
  useProviders as E,
  signal as A
} from "runtime";

const
  a = (b) => {
    return b
  },
  c = A([]),
  B = C(c, async () => {
    return await null
  }),
  D = () => {
    E([[() => {
      B({
        test: ``
      })
    }]]);
    c.value;
    return a(`a`)
  };
