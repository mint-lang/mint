global component Test {
  const NAME = "ASD"

  fun render : String {
    NAME
  }
}

component Main {
  fun render : Html {
    <div>
      <{ Test:NAME }>
    </div>
  }
}
--------------------------------------------------------------------------------
import { createElement as C } from "runtime";

const a = `ASD`;

const A = () => {
  return a
};

const B = () => {
  return C("div", {}, [a])
};
