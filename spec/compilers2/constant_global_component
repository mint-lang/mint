global component Test {
  const NAME = "ASD"

  fun render : String {
    NAME
  }
}

component Main {
  fun render : Html {
    <div>
      <{ Test:NAME }>
    </div>
  }
}
--------------------------------------------------------------------------------
import { createElement as C } from "runtime";

const
  a = `ASD`,
  A = () => {
    return a
  },
  B = () => {
    return C("div", {}, [a])
  };
