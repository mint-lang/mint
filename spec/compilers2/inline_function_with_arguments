component Main {
  fun test : String {
    let getName =
      (name : String) : String { name }

    getName("Joe")
  }

  fun render : String {
    test()

    ""
  }
}
--------------------------------------------------------------------------------
import { useFunction as B } from "runtime";

const A = () => {
  const a = B(() => {
    const b = (c) => {
      return c
    };
    return b(`Joe`)
  });
  a();
  return ``
};
