component Main {
  fun a : String {
    "test"
  }

  fun test : String {
    a()
  }

  fun render : String {
    test()

    ""
  }
}
--------------------------------------------------------------------------------
import { useFunction as B } from "runtime";

const A = () => {
  const
    a = B(() => {
      return `test`
    }),
    b = B(() => {
      return a()
    });
  b();
  return ``
};
